{
  "description": "Interface d'administration du noeud RUDI",
  "name": "rudi-prodmanager-front",
  "version": "2.3.1",
  "author": "Yann Porret <yporret@micropole.com>, Olivier Martineau <olivier.martineau@irisa.fr>",
  "license": "EUPL-1.2",
  "private": true,
  "scripts": {
    "prestart": "npm run install:dev",
    "install:dev": "NODE_ENV=development npm i --loglevel=error",
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "npm run build:prod",
    "build:dev": "DISABLE_ESLINT_PLUGIN=true INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false  NODE_ENV=development react-scripts build",
    "build:prod": "DISABLE_ESLINT_PLUGIN=true INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build",
    "build:local": "PUBLIC_URL='' DISABLE_ESLINT_PLUGIN=true INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false NODE_ENV=production react-scripts build",
    "build:windows": "Set DISABLE_ESLINT_PLUGIN=true & Set INLINE_RUNTIME_CHUNK=false & react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**.js",
    "cleanup": "rm -fR ./node_modules; rm -fR ./package-lock.json; rm -fR ./yarn.lock"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@svgr/webpack": "^8.1.0",
    "@textea/json-viewer": "4.0.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "history": "^5.3.0",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "jspreadsheet-ce": "^4.15.0",
    "nth-check": "^2.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.5",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.80.6",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3"
  },
  "overrides": {
    "@svgr/webpack": "$@svgr/webpack",
    "nth-check": "$nth-check",
    "postcss": "$postcss"
  }
}
